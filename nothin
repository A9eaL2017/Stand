local Toggle2 = Section:NewToggle(" Auto Claim  ", "", function(state)
    scriptEnabled2 = state
    if scriptEnabled2 then
        EnableScript2()
    else
        DisableScript2()
    end
end)

function EnableScript2()
    while scriptEnabled2 do
        wait(0)


-- Get the LocalPlayer
local localPlayer = game:GetService("Players").LocalPlayer

-- Function to delete BoolValues directly under LocalPlayer
local function deleteBoolValues()
    for _, child in pairs(localPlayer:GetChildren()) do
        if child:IsA("BoolValue") and child.Name ~= "isShiftLock" then
            child:Destroy()
        end
    end
end

-- Call the function to delete BoolValues in LocalPlayer
deleteBoolValues()




        local itemFolder = workspace:WaitForChild("Item")

local function triggerProximityPrompt(object)
    local proximityPrompt = object:FindFirstChildOfClass("ProximityPrompt")
    if proximityPrompt and proximityPrompt:IsA("ProximityPrompt") then
        proximityPrompt:InputHoldBegin()
    end
end

local itemNames = {"Chest", "Box", "BoxDrop1", "SukunaFinger", "BoxDrop2", "BoxDrop3", "BoxDrop4", "Barrel"}

-- Iterate through the children of the Item folder
for _, child in pairs(itemFolder:GetChildren()) do
    -- Check if the child is a BasePart and its name is in the list
    if child:IsA("BasePart") and table.find(itemNames, child.Name) then
        -- Trigger the ProximityPrompt for each matching child
        triggerProximityPrompt(child)
    end
end



    end        
end

function DisableScript2()
    print("Script disabled")
end

