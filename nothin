local Toggle99 = Section:NewToggle("Auto Tp To Dummy", "This Will Tp To Finger When Spawned", function(state)
    scriptEnabled99 = state
    if scriptEnabled99 then
        EnableScript99()
    else
        DisableScript99()
    end
end)

function EnableScript99()
    while scriptEnabled99 do
        wait(0)

        local function teleportToBackOfDummy(dummy)
            if dummy and dummy:IsA("Model") then
                local humanoidRootPart = dummy:FindFirstChild("HumanoidRootPart")
        
                if humanoidRootPart then
                    local backOffset = CFrame.new(0, 0, 4) -- You can adjust the offset as needed
        
                    local newCFrame = humanoidRootPart.CFrame * backOffset
                    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(newCFrame)
                else
                    warn("HumanoidRootPart not found in Dummy.")
                end
            else
                warn("Invalid Dummy.")
            end
        end
        
        -- Find the Living folder in workspace
        local livingFolder = workspace:FindFirstChild("Living")
        
        -- Check if the folder exists
        if livingFolder and livingFolder:IsA("Folder") then
            -- Find the first child in the Living folder named "Dummy"
            local dummy = livingFolder:FindFirstChild("Dummy")
        
            -- Teleport to the back of the Dummy
            teleportToBackOfDummy(dummy)
        else
            warn("Living folder not found in workspace.")
        end
        
    end
end


function DisableScript99()
    print("Script disabled")
end


local Toggle72 = Section:NewToggle("Auto Attacking Dummy", "", function(state)
    scriptEnabled72 = state
    if scriptEnabled72 then
        EnableScript72()
    else
        DisableScript72()
    end
end)

function EnableScript72()
    while scriptEnabled72 do
        wait(0)

        local function teleportToBackOfCharacter(character)
            if character and character:IsA("Model") then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        
                if humanoidRootPart then
                    local backOffset = CFrame.new(0, 0, 0) -- You can adjust the offset as needed
        
                    local newCFrame = humanoidRootPart.CFrame * backOffset
                    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(newCFrame)
                else
                    warn("HumanoidRootPart not found in Character.")
                end
            else
                warn("Invalid Character.")
            end
        end
        
        -- Find the Living folder in workspace
        local livingFolder = workspace:FindFirstChild("Living")
        
        -- Check if the folder exists
        if livingFolder and livingFolder:IsA("Folder") then
            -- Find the first child in the Living folder named "Attacking Dummy"
            local attackingDummyCharacter = livingFolder:FindFirstChild("Attacking Dummy")
        
            -- Teleport to the back of the Attacking Dummy
            teleportToBackOfCharacter(attackingDummyCharacter)
        else
            warn("Living folder not found in workspace.")
        end
        
        
    end
end


function DisableScript72()
    print("Script disabled")
end



