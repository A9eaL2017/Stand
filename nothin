local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("a9eal's | discord.com/invite/njACdJ4XEV | Join For Updates !", "Midnight")
local Tab = Window:NewTab("Items")
local Section = Tab:NewSection("Items")


local Toggle47 = Section:NewToggle("Auto Items", "", function(state)
    scriptEnabled47 = state
    if scriptEnabled47 then
        EnableScript47()
    else
        DisableScript47()
    end
end)

function EnableScript47()
    while scriptEnabled47 do
        wait(0)
        local function teleportToCFrame(object)
            if object:IsA("BasePart") then
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(object.CFrame)
            end
        end
        
        -- Define the target CFrame to ignore
        local targetCFrame = CFrame.new(1804.53906, -298.173584, -309.464966, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        
        -- Find the Item folder in workspace
        local itemFolder = workspace:FindFirstChild("Item")
        
        -- Check if the folder exists
        if itemFolder and itemFolder:IsA("Folder") then
            -- Loop through the objects in the Item folder
            for _, object in pairs(itemFolder:GetChildren()) do
                -- Check if the object is named "Box" and has an object called "Outline" inside
                local outlineObject = object:FindFirstChild("Outline")
                if object:IsA("BasePart") and object.Name == "Box" and outlineObject and object.CFrame ~= targetCFrame then
                    -- Teleport to the CFrame of the Box object
                    teleportToCFrame(object)
                end
            end
        else
            warn("Item folder not found in workspace.")
        end


        
    end
end


function DisableScript47()
    print("Script disabled")
end


