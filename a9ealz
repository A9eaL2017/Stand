local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("a9eal's | discord.com/invite/njACdJ4XEV | Join For Updates !", "Midnight")
local Tab = Window:NewTab("Items")
local Section = Tab:NewSection("Items")

local Toggle1 = Section:NewToggle("Auto Box", "ToggleInfo", function(state)
    scriptEnabled1 = state
    if scriptEnabled1 then
        EnableScript1()
    else
        DisableScript1()
    end
end)

function EnableScript1()
    while scriptEnabled1 do
        wait(0)

    
        local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local workspace = game:GetService("Workspace")

local function teleportToCFrame(object)
    if object:IsA("BasePart") then
        -- Create a TweenInfo object for the tween
        local tweenInfo = TweenInfo.new(
            3, -- Duration
            Enum.EasingStyle.Linear, -- Easing style
            Enum.EasingDirection.In, -- Easing direction
            0, -- Number of times to repeat (0 means no repeat)
            false, -- Reverses the tween if true
            0 -- Delay before starting the tween
        )	

        -- Define the target position
        local targetPosition = object.Position

        -- Create the tween
        local tween = TweenService:Create(
            Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), -- Object to tween
            tweenInfo,
            {CFrame = CFrame.new(targetPosition)} -- Target properties
        )

        -- Play the tween
        tween:Play()
    end
end

-- Define the target CFrame to ignore
local targetCFrame = CFrame.new(1804.53906, -298.173584, -309.464966, 1, 0, 0, 0, 1, 0, 0, 0, 1)

-- Find the Item folder in workspace
local itemFolder = workspace:FindFirstChild("Item")

-- Check if the folder exists
if itemFolder and itemFolder:IsA("Folder") then
    -- Loop through the objects in the Item folder
    for _, object in pairs(itemFolder:GetChildren()) do
        -- Check if the object is named "Box" and has an object called "Outline" inside
        local outlineObject = object:FindFirstChild("Outline")
        if object:IsA("BasePart") and object.Name == "Box" and outlineObject and object.CFrame ~= targetCFrame then
            -- Teleport to the CFrame of the Box object using tween
            teleportToCFrame(object)
        end
    end
else
    warn("Item folder not found in workspace.")
end

        
        
wait(1)

game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.E, false, game)


wait(2)

game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.E, false, game)

        




    end
    

 end

function DisableScript1()
    print("Script disabled")
end




local Toggle12 = Section:NewToggle("Auto Chests", "must have key", function(state)
    scriptEnabled12 = state
    if scriptEnabled12 then
        EnableScript12()
    else
        DisableScript12()
    end
end)

function EnableScript12()
    while scriptEnabled12 do
        wait(1)
        local function teleportToChest(object)
            if object:IsA("BasePart") then
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(object.CFrame)
            end
        end
        
        -- Find the Item folder in workspace
        local itemFolder = workspace:FindFirstChild("Item")
        
        -- Check if the folder exists
        if itemFolder and itemFolder:IsA("Folder") then
            -- Loop through the objects in the Item folder
            for _, object in pairs(itemFolder:GetChildren()) do
                -- Check if the object is named "Chest"
                if object:IsA("BasePart") and object.Name == "Chest" then
                    -- Teleport to the CFrame of the Chest object
                    teleportToChest(object)
                end
            end
        else
            warn("Item folder not found in workspace.")
        end

        local itemFolder = workspace.Item

-- Function to trigger the ProximityPrompt of a given object
local function triggerProximityPrompt(object)
    local proximityPrompt = object:FindFirstChildOfClass("ProximityPrompt")
    if proximityPrompt and proximityPrompt:IsA("ProximityPrompt") then
        proximityPrompt:InputHoldBegin()  -- You can use other methods like InputBegin or TriggerPrompt based on your needs
    end
end

-- Iterate through the children of the Item folder
for _, child in pairs(itemFolder:GetChildren()) do
    -- Check if the child is a BasePart and its name is "Chest"
    if child:IsA("BasePart") and child.Name == "Chest" then
        -- Trigger the ProximityPrompt for each matching child
        triggerProximityPrompt(child)
    end
end


        
    end
end


function DisableScript12()
    print("Script disabled")
end

