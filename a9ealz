local Toggle2 = Section:NewToggle("Instant Claim Boxes Only", "Put Skills On Z,X,C,V,B,N", function(state)
    scriptEnabled2 = state
    if scriptEnabled2 then
        EnableScript2()
    else
        DisableScript2()
    end
end)

function EnableScript2()
    while scriptEnabled2 do
        wait(0.1)

        local itemFolder = workspace.Item

-- Function to trigger the ProximityPrompt of a given object
local function triggerProximityPrompt(object)
    local proximityPrompt = object:FindFirstChildOfClass("ProximityPrompt")
    if proximityPrompt and proximityPrompt:IsA("ProximityPrompt") then
        proximityPrompt:InputHoldBegin()  -- You can use other methods like InputBegin or TriggerPrompt based on your needs
    end
end

-- Iterate through the children of the Item folder
for _, child in pairs(itemFolder:GetChildren()) do
    -- Check if the child is a BasePart and its name contains "Box"
    if child:IsA("BasePart") and string.find(child.Name, "Box") then
        -- Trigger the ProximityPrompt for each matching child
        triggerProximityPrompt(child)
    end
end

        
    end
end


function DisableScript2()
    print("Script disabled")
end


local Toggle3 = Section:NewToggle("Esp Items", "ToggleInfo", function(state)
    scriptEnabled3 = state
    if scriptEnabled3 then
        EnableScript3()
    else
        DisableScript3()
    end
end)

function EnableScript3()
    while scriptEnabled3 do
        wait(3)

        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")
        local Workspace = game:GetService("Workspace")
        local LocalPlayer = Players.LocalPlayer
        
        -- Function to create a BillboardGui for an item
        local function createESP(item)
            local billboardGui = Instance.new("BillboardGui")
            billboardGui.Adornee = item
            billboardGui.Size = UDim2.new(0, 100, 0, 50)
            billboardGui.StudsOffset = Vector3.new(0, 3, 0)
            billboardGui.AlwaysOnTop = true
        
            local textLabel = Instance.new("TextLabel")
            textLabel.Size = UDim2.new(0, 100, 0, 50)
            textLabel.Text = item.Name
            textLabel.TextColor3 = Color3.new(1, 1, 1)
            textLabel.Font = Enum.Font.SourceSansBold
            textLabel.FontSize = Enum.FontSize.Size24
            textLabel.BackgroundTransparency = 1
            textLabel.TextStrokeTransparency = 0.5
            textLabel.TextStrokeColor3 = Color3.new(0, 0, 0)
        
            textLabel.Parent = billboardGui
            billboardGui.Parent = Workspace.CurrentCamera
        end
        
        -- Function to create ESP for all items in the Item folder
        local function createAllESP()
            local itemsFolder = Workspace:FindFirstChild("Item")
        
            if itemsFolder and itemsFolder:IsA("Folder") then
                for _, item in pairs(itemsFolder:GetChildren()) do
                    if item:IsA("BasePart") then
                        createESP(item)
                    end
                end
            else
                warn("Item folder not found in workspace.")
            end
        end
        
        -- Create ESP for existing items
        createAllESP()
        
        -- Create ESP for new items added to the folder
        Workspace.ChildAdded:Connect(function(child)
            if child:IsA("BasePart") and child.Parent.Name == "Item" then
                createESP(child)
            end
        end)
        
        -- Remove ESP for items that are removed from the folder
        Workspace.ChildRemoved:Connect(function(child)
            if child:IsA("BasePart") and child.Parent.Name == "Item" then
                for _, descendant in pairs(Workspace.CurrentCamera:GetChildren()) do
                    if descendant.Adornee == child then
                        descendant:Destroy()
                    end
                end
            end
        end)
        
        -- Update ESP positions
        RunService.RenderStepped:Connect(function()
            for _, descendant in pairs(Workspace.CurrentCamera:GetChildren()) do
                if descendant:IsA("BillboardGui") then
                    local adornee = descendant.Adornee
                    if adornee and adornee.Parent then
                        local screenPos, onScreen = Workspace.CurrentCamera:WorldToScreenPoint(adornee.Position)
                        if onScreen then
                            descendant.Enabled = true
                            descendant.Position = UDim2.new(0, screenPos.X, 0, screenPos.Y - 50)
                        else
                            descendant.Enabled = false
                        end
                    else
                        descendant:Destroy()
                    end
                end
            end
        end)
        


    end
end

function DisableScript3()
    print("Script disabled")
end


